@model IEnumerable<GameCafe.Storage.Entity.Place>


<body class="shared_design">
    <img 
    style="margin-bottom:64px;width:500px;    
    display: block;
    margin-left: auto;
    margin-right: auto;"
    class="inLogo" 
    src="img/splashes/visit.png"
    >
@{

    foreach (var item in Model)
    {
        <div class="submenu_card">
                                <div class="card" style="width: 100%">
                                    <img src="@item.GeoPic" class="card-img-top" alt="">
                                    <div class="card-body">
                                    <h5 class="card-title">@item.Address</h5>
                                    </div>
                                    <ul class="list-group list-group-flush">
                                    <li class="list-group-item">@item.Phone</li>
                                    </ul>
                                </div>
                                </div>

    }
}
</body>